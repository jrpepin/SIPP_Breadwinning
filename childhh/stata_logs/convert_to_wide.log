. * Section: Create Race/Ethnicity variables (2 versions) and set my_race 
. *           (and my_race_v2) equal to first observation. 
. ***********************************************************************
. 
. #delimit ;
delimiter now ;
. label define race   1 "NH white"
>                     2 "black"
>                     3 "non-black Hispanic"
>                     4 "NH Asian"
>                     5 "NH other";

. label define racealt  1 "NH white"
>                                         2 "NH black"
>                                         3 "Hispanic"
>                                         4 "NH Asian"
>                                         5 "NH Other";

. #delimit cr
delimiter now cr
. 
. * there was a variable for race in each wave
. forvalues wave = $first_wave/$final_wave {
  2.     recode erace`wave' (1=1) (2=2) (3=4) (4=5), generate (race`wave')
  3.     replace race`wave' = 3 if ((eorigin`wave' == 1) & (erace`wave' != 2)) /* non-black Hispanic */
  4.         recode erace`wave' (1=1)(2=2)(3=4)(4=5), generate(racealt`wave')
  5.         replace racealt`wave' = 3 if eorigin`wave'==1 /*All Hispanic */
  6.     label values race`wave' race
  7.         label values racealt`wave' racealt
  8. }
